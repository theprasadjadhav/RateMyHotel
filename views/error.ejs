<%- layout("./layout/boilerplate.ejs") -%>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading">
                <%= err.status %>
            </h4>
            <p>
                <%= err.message%>
            </p>
            <p>
                <%= err.stack %>
            </p>
        </div>

    </div>
</div>